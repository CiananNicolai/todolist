<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:f="sap.ui.layout.form"
		xmlns:core="sap.ui.core"
        xmlns:u="sap.ui.unified"
        xmlns:l="sap.ui.layout"
        >
	<ResponsivePopover
			id="importPopover"
			title="Import"
			class="sapUiResponsivePadding--header"
			placement="Auto"
            width="100%">
	<l:VerticalLayout 
    class="sapUiContentPadding"
	width="100%">
		<u:FileUploader
			id="fileUploader"
			name="myFileUpload"
			uploadUrl="../webserver/model"
			tooltip="Upload your calender dates!"
			uploadComplete="handleUploadComplete"/>
		<Button
			text="Upload File"
			press="handleUploadPress"/>

        

        
	</l:VerticalLayout>
	</ResponsivePopover>
</core:FragmentDefinition>
